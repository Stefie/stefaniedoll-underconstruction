@import "_variables"

@font-face
  font-family 'iconfont'
  src url(icomoon-font-path + '/iconfont.ttf?lexu2l') format('truetype'), url(icomoon-font-path + '/iconfont.woff?lexu2l') format('woff'), url(icomoon-font-path + '/iconfont.svg?lexu2l#iconfont') format('svg')
  font-weight normal
  font-style normal

[class^="icon-"], [class*=" icon-"]
  font-family 'iconfont' !important
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1

  /* Better Font Rendering =========== */
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

