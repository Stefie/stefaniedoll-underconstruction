@import "_variables"

@font-face
  font-family 'iconfont'
  src url(icomoon-font-path + '/iconfont.ttf?5fgxz3') format('truetype'), url(icomoon-font-path + '/iconfont.woff?5fgxz3') format('woff'), url(icomoon-font-path + '/iconfont.svg?5fgxz3#iconfont') format('svg')
  font-weight normal
  font-style normal

[class^="icon-"], [class*=" icon-"], .icon
  font-family 'iconfont' !important
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1

  /* Better Font Rendering =========== */
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

.icon-stickman-1
  &:before
    content icon-stickman-1
.icon-stickman-2
  &:before
    content icon-stickman-2
.icon-github
  &:before
    content icon-github
.icon-twitter-with-circle
  &:before
    content icon-twitter-with-circle
.icon-twitter
  &:before
    content icon-twitter
.icon-linkedin
  &:before
    content icon-linkedin
.icon-linkedin2
  &:before
    content icon-linkedin2
.icon-xing
  &:before
    content icon-xing
.icon-xing2
  &:before
    content icon-xing2
.icon-mark-github
  &:before
    content icon-mark-github
